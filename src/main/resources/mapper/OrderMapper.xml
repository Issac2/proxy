        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.minuor.aop.dao.OrderDao">


    <insert id="insert">
        insert into t_order_info
        (order_id,user_id,phone,amount)
        values
        (#{model.orderId},#{model.userId},#{model.phone},#{model.amount})
    </insert>
</mapper>